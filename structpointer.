#include<stdio.h>
struct stu
{
char name[50];
int marks;
};

void main()
{
struct stu s[5];
int i;
for(i=0;i<5;i++)
{
printf("\nenter name:");
scanf("%c",&s[i].name);
printf("\nEnter the roll number");
scanf("%d",&s[i].marks);
}

for(i=0;i<5;i++)
{
printf("\nname=%c\nmarks=%d",*s[i].name,*s[i].marks);
}
}
